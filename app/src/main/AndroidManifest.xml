<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="suza.project.crazyballs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/smiley_face"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="suza.project.crazyballs.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="suza.project.crazyballs.GameActivity" />
        <activity android:name="suza.project.crazyballs.HowToPlayActivity" />
        <activity android:name="suza.project.crazyballs.ResultActivity" />
        <activity android:name="suza.project.crazyballs.ScoreActivity"></activity>
    </application>

</manifest>
